<div class="head_create_news">
  <h2 class ="fornow">Make some news</h2>
</div><!--head_create_news-->

<div class="news_form">

  <%= form_for :article do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>

  <p>
    <%= f.submit %>
  </p>
  <% end %>

</div><!--news_form-->
